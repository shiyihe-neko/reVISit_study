{
  "$schema": "https://raw.githubusercontent.com/revisit-studies/study/v2.0.0-rc5/src/parser/LibraryConfigSchema.json",
  "baseComponents": {
    "test-baseComponent": {
      "type": "markdown",
      "path": "libraries/test/assets/test.md",
      "response": []
    }
  },
  "components": {
    "test-react": {
      "type": "react-component",
      "path": "libraries/test/assets/test.tsx",
      "response": []
    },
    "test-md": {
      "type": "markdown",
      "path": "libraries/test/assets/test.md",
      "response": []
    },
    "test-inherited": {
      "baseComponent": "test-baseComponent",
      "response": [{
        "id": "test-response",
        "type": "shortText",
        "prompt": "What did you think of the test?",
        "required": false
      }]
    }
  },
  "sequences": {
    "small": {
      "order": "fixed",
      "components": [
        "test-md"
      ]
    },
    "long": {
      "order": "fixed",
      "components": [
        "test-react",
        "test-md",
        "test-inherited"
      ]
    }
  }
}
